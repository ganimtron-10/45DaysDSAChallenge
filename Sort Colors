https://leetcode.com/problems/sort-colors/description/

func sortColors(nums []int)  {
    var l, m, h int
    h = len(nums) - 1

    for m <= h{
        if(nums[m] == 0){
            temp := nums[m]
            nums[m] = nums[l]
            nums[l] = temp
            m++
            l++
        }else if(nums[m] == 1){
            m++
        }else{
            temp := nums[h]
            nums[h] = nums[m]
            nums[m] = temp
            h--
        }
    }

}